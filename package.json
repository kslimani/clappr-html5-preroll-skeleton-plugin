{
  "name": "clappr-html5-preroll-skeleton-plugin",
  "version": "0.1.1",
  "description": "HTML5 preroll ad skeleton plugin for clappr.",
  "main": "dist/clappr-html5-preroll-plugin.js",
  "scripts": {
    "dist": "npm run dist-main && npm run dist-min",
    "dist-main": "webpack --progress",
    "dist-min": "webpack --progress -p --optimize-mangle --optimize-dedupe --output-filename clappr-html5-preroll-plugin.min.js",
    "watch": "webpack -w",
    "demo": "http-cli -r ./ -a 0.0.0.0 -p 8090",
    "check": "david"
  },
  "author": "Karim Slimani <karim@playmedia.fr>",
  "license": "MIT",
  "repository": "kslimani/clappr-html5-preroll-skeleton-plugin",
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.18.0",
    "clean-webpack-plugin": "0.1.14",
    "compass-mixins": "0.12.10",
    "css-loader": "0.25.0",
    "david": "9.0.0",
    "http-cli": "1.1.0",
    "json-loader": "0.5.4",
    "node-sass": "9.0.0",
    "sass-loader": "4.0.2",
    "svg-inline-loader": "0.7.1",
    "webpack": "5.94.0"
  }
}
